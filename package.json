{
  "name": "au_life_server",
  "version": "0.0.0",
  "description": "seerver for au_life",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "commit": "git-cz",
    "commit-all": "git add . && git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kalinnikov2n/au_life_server.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "express",
    "socket.io",
    "typescript"
  ],
  "author": "Kalinnikov Nikita <https://github.com/Kalinnikov2n>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kalinnikov2n/au_life_server/issues"
  },
  "homepage": "https://github.com/Kalinnikov2n/au_life_server#readme",
  "dependencies": {
    "@types/express": "^4.17.9",
    "commitizen": "^4.2.2",
    "express": "^4.17.1",
    "git-cz": "^4.7.6",
    "http": "0.0.1-security",
    "socket.io": "^3.0.4",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1"
  }
}
